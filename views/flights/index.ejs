<%- include('../partials/header') %>

<h2 id="current-flights">Current Flights</h2>
<table>
  <thead>
    <tr>
      <th>Airline</th>
      <th>Airport</th>
      <th>Flight Number</th>
      <th>Departs</th>
    </tr>
  </thead>
  <tbody>
    <% flights.forEach(f => {   %>

      <tr>
        <td><%= f.airline %></td>
        <td><%= f.airport %></td>
        <td><%= f.flightNo %></td>
        <td><%= f.departs %></td>
      </tr>

    <% }); %>
  </tbody>
</table>

<%- include('../partials/footer') %>